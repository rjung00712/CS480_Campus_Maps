<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cs499android.com.cppcampusmap.MapsActivity"
    tools:showIn="@layout/app_bar_main">

    <com.arlib.floatingsearchview.FloatingSearchView
        android:id="@+id/floating_search_view"
        android:layout_width="368dp"
        android:layout_height="439dp"
        floatingSearch_menu="@menu/menu_search_view"
        floatingSearch_searchBarMarginLeft="@dimen/search_view_inset"
        floatingSearch_searchBarMarginRight="@dimen/search_view_inset"
        floatingSearch_searchBarMarginTop="@dimen/search_view_inset"
        floatingSearch_searchHint="Search..."
        floatingSearch_suggestionsListAnimDuration="250"
        floatingSearch_showSearchKey="true"
        floatingSearch_leftActionMode="showHamburger"
        floatingSearch_close_search_on_keyboard_dismiss="true"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp" />
</RelativeLayout>